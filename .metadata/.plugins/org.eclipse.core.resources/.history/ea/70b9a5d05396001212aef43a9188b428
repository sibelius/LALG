PARSER_BEGIN(MyParser)public class MyParser {	public static void main(String[] args)	{		MyParser parser = new MyParser(System.in);		while(true)		{			parser.Prog();		}	}}PARSER_END(MyParser)SKIP :{ " " | "\t" | "\n" }TOKEN :{ < WHILE: "while" >| < BEGIN: "begin" >| < END: "end" >| < DO: "do" >| < IF: "if" >| < THEN: "then" >| < ELSE: "else" >| < SEMI: ";" >| < ASSIGN: "=" >| < ID: ["a"-"z"](["a"-"z"] | ["0"-"9"])* >}void Prog() :{}{ StmList() <EOF> }void StmList() :{}{ Stm() StmListPrime() }void StmListPrime() :{}{ ( ";" Stm() StmListPrime() )? }void Stm() :{}{ <ID> "=" <ID>| "while" <ID> "do" Stm()| "begin" StmList() "end"| LOOKAHEAD(5) /* we need to lookahead till we see ”else” */"if" <ID> "then" Stm()| "if" <ID> "then" Stm() "else" Stm()}